        }

        // –ü—Ä–æ–≤–µ—Ä–∫–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–π
        function checkForUpdates() {
            if (settings.autoUpdate === 'off') {
                logMessage("–ê–≤—Ç–æ–æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ –æ—Ç–∫–ª—é—á–µ–Ω–æ –≤ –Ω–∞—Å—Ç—Ä–æ–π–∫–∞—Ö");
                return;
            }
            
            logMessage("–ü—Ä–æ–≤–µ—Ä–∫–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–π...", "info");
            
            try {
                // –ò–º–∏—Ç–∞—Ü–∏—è –ø—Ä–æ–≤–µ—Ä–∫–∏ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–π —á–µ—Ä–µ–∑ GitHub
                // –í —Ä–µ–∞–ª—å–Ω–æ–π —Ä–µ–∞–ª–∏–∑–∞—Ü–∏–∏ –∑–¥–µ—Å—å –±—É–¥–µ—Ç –∑–∞–ø—Ä–æ—Å –∫ GitHub API
                var currentVersion = "2.8";
                var latestVersion = "2.9"; // –ò–º–∏—Ç–∞—Ü–∏—è –Ω–æ–≤–æ–π –≤–µ—Ä—Å–∏–∏
                
                if (compareVersions(latestVersion, currentVersion) > 0) {
                    logMessage("–û–±–Ω–∞—Ä—É–∂–µ–Ω–∞ –Ω–æ–≤–∞—è –≤–µ—Ä—Å–∏—è: " + latestVersion, "info");
                    showUpdateModal(latestVersion);
                } else {
                    logMessage("–£—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∞ –ø–æ—Å–ª–µ–¥–Ω—è—è –≤–µ—Ä—Å–∏—è", "success");
                }
            } catch (e) {
                logMessage("–û—à–∏–±–∫–∞ –ø—Ä–æ–≤–µ—Ä–∫–∏ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–π: " + e.message, "error");
            }
        }

        // –°—Ä–∞–≤–Ω–µ–Ω–∏–µ –≤–µ—Ä—Å–∏–π
        function compareVersions(version1, version2) {
            var v1 = version1.split('.').map(Number);
            var v2 = version2.split('.').map(Number);
            
            for (var i = 0; i < Math.max(v1.length, v2.length); i++) {
                var num1 = v1[i] || 0;
                var num2 = v2[i] || 0;
                
                if (num1 > num2) return 1;
                if (num1 < num2) return -1;
            }
            
            return 0;
        }

        // –ü–æ–∫–∞–∑–∞—Ç—å –æ–∫–Ω–æ –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è
        function showUpdateModal(newVersion) {
            document.getElementById('updateModal').style.display = 'block';
            logMessage("–û—Ç–∫—Ä—ã—Ç–æ –æ–∫–Ω–æ –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è –¥–æ –≤–µ—Ä—Å–∏–∏ " + newVersion, "info");
        }

        // –ù–∞—á–∞—Ç—å –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ
        function startUpdate() {
            var installBtn = document.getElementById('updateInstallBtn');
            var skipBtn = document.getElementById('updateSkipBtn');
            var progressBar = document.getElementById('updateProgressBar');
            var progressText = document.getElementById('updateProgressText');
            
            installBtn.disabled = true;
            skipBtn.disabled = true;
            installBtn.innerText = "üì• –£—Å—Ç–∞–Ω–æ–≤–∫–∞...";
            
            logMessage("–ù–∞—á–∞–ª–æ —É—Å—Ç–∞–Ω–æ–≤–∫–∏ –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è...", "info");
            
            // –ò–º–∏—Ç–∞—Ü–∏—è –ø—Ä–æ—Ü–µ—Å—Å–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è
            var progress = 0;
            var updateInterval = setInterval(function() {
                progress += Math.random() * 10;
                if (progress > 100) progress = 100;
                
                progressBar.style.width = progress + '%';
                progressText.innerText = "–£—Å—Ç–∞–Ω–æ–≤–∫–∞: " + Math.round(progress) + "%";
                
                if (progress === 100) {
                    clearInterval(updateInterval);
                    installBtn.innerText = "‚úÖ –£—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–æ!";
                    progressText.innerText = "–û–±–Ω–æ–≤–ª–µ–Ω–∏–µ —É—Å–ø–µ—à–Ω–æ —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–æ!";
                    
                    logMessage("–û–±–Ω–æ–≤–ª–µ–Ω–∏–µ —É—Å–ø–µ—à–Ω–æ —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–æ", "success");
                    
                    // –ü–µ—Ä–µ–∑–∞–ø—É—Å–∫ –ø—Ä–æ–≥—Ä–∞–º–º—ã —á–µ—Ä–µ–∑ 2 —Å–µ–∫—É–Ω–¥—ã
                    setTimeout(function() {
                        logMessage("–ü–µ—Ä–µ–∑–∞–ø—É—Å–∫ –ø—Ä–æ–≥—Ä–∞–º–º—ã...", "info");
                        showNotification("–û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∑–∞–≤–µ—Ä—à–µ–Ω–æ", "–ü—Ä–æ–≥—Ä–∞–º–º–∞ –±—É–¥–µ—Ç –ø–µ—Ä–µ–∑–∞–ø—É—â–µ–Ω–∞", "success");
                        
                        // –í —Ä–µ–∞–ª—å–Ω–æ–π —Ä–µ–∞–ª–∏–∑–∞—Ü–∏–∏ –∑–¥–µ—Å—å –±—É–¥–µ—Ç –ø–µ—Ä–µ–∑–∞–ø—É—Å–∫ –ø—Ä–æ–≥—Ä–∞–º–º—ã
                        setTimeout(function() {
                            // –ò–º–∏—Ç–∞—Ü–∏—è –ø–µ—Ä–µ–∑–∞–ø—É—Å–∫–∞
                            document.getElementById('updateModal').style.display = 'none';
                            installBtn.disabled = false;
                            skipBtn.disabled = false;
                            installBtn.innerText = "üì• –£—Å—Ç–∞–Ω–æ–≤–∏—Ç—å –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ";
                            progressBar.style.width = '0%';
                            progressText.innerText = "–ì–æ—Ç–æ–≤–æ –∫ —É—Å—Ç–∞–Ω–æ–≤–∫–µ";
                            
                            logMessage("–ü—Ä–æ–≥—Ä–∞–º–º–∞ –ø–µ—Ä–µ–∑–∞–ø—É—â–µ–Ω–∞ —Å –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ–º", "success");
                        }, 2000);
                    }, 2000);
                }
            }, 200);
        }

        // –ü—Ä–æ–ø—É—Å—Ç–∏—Ç—å –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ
        function skipUpdate() {
