                document.getElementById('status').innerText = '–û—à–∏–±–∫–∞ PRO –ø–æ–¥–∫–ª—é—á–µ–Ω–∏—è';
                isRunning = false;
                document.getElementById('connectBtn').disabled = false;
                document.getElementById('proConnectBtn').disabled = false;
                document.getElementById('disconnectBtn').disabled = true;
                document.getElementById('loading').style.display = 'none';
                document.querySelector('.loading-dots').classList.remove('pro');
                showNotification("–û—à–∏–±–∫–∞ PRO", "–ù–µ —É–¥–∞–ª–æ—Å—å –∑–∞–ø—É—Å—Ç–∏—Ç—å PRO –æ–±—Ö–æ–¥: " + e.message, "error");
            }
        }
        
        // –û—Ç–∫–ª—é—á–µ–Ω–∏–µ
        function disconnectBypass() {
            if (!isRunning) return;
            
            isRunning = false;
            document.getElementById('status').className = 'status';
            document.getElementById('status').innerText = '–û—Ç–∫–ª—é—á–µ–Ω–∏–µ...';
            
            try {
                shell.Run('taskkill /f /im cmd.exe', 0, false);
                logMessage('–ü—Ä–æ—Ü–µ—Å—Å—ã –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω—ã', "success");
            } catch (e) {
                logMessage('–û—à–∏–±–∫–∞ –ø—Ä–∏ –æ—Ç–∫–ª—é—á–µ–Ω–∏–∏: ' + e.message, "error");
            }
            
            setTimeout(function() {
                document.getElementById('status').innerText = '–ì–æ—Ç–æ–≤–æ –∫ –ø–æ–¥–∫–ª—é—á–µ–Ω–∏—é';
                document.getElementById('connectBtn').disabled = false;
                document.getElementById('proConnectBtn').disabled = false;
                document.getElementById('disconnectBtn').disabled = true;
                document.getElementById('loading').style.display = 'none';
                document.querySelector('.loading-dots').classList.remove('pro');
                logMessage('–û–±—Ö–æ–¥ –æ—Ç–∫–ª—é—á–µ–Ω');
                showNotification("–û—Ç–∫–ª—é—á–µ–Ω–æ", "–ü–æ–¥–∫–ª—é—á–µ–Ω–∏–µ –∫ —Å–µ—Ä–≤–µ—Ä–∞–º –ø—Ä–µ–∫—Ä–∞—â–µ–Ω–æ.", "info");
            }, 2000);
        }

        // –ü–æ–∫—É–ø–∫–∞ PRO
        function buyPro() {
            shell.Run("https://slava20222014-create.github.io/Gemer-Presents.com/");
            logMessage("–û—Ç–∫—Ä—ã—Ç–∞ —Å—Ç—Ä–∞–Ω–∏—Ü–∞ –ø–æ–∫—É–ø–∫–∏ PRO", "info");
            showNotification("–ü–µ—Ä–µ—Ö–æ–¥ –Ω–∞ —Å–∞–π—Ç", "–û—Ç–∫—Ä—ã—Ç–∞ —Å—Ç—Ä–∞–Ω–∏—Ü–∞ –ø–æ–∫—É–ø–∫–∏ PRO –≤–µ—Ä—Å–∏–∏", "info");
        }

        // –ê–∫—Ç–∏–≤–∞—Ü–∏—è PRO –∫–ª—é—á–∞
        function activateProKey() {
            var proKey = document.getElementById('proKey').value;
            
            // –ü—Ä–æ–≤–µ—Ä—è–µ–º –∫–ª—é—á –Ω–∞ —Å–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤–∏–µ —Ñ–æ—Ä–º–∞—Ç—É –∏ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –∞–∫—Ç–∏–≤–∞—Ü–∏–π
            if (proKey && proKey.match(/^[A-Z0-9]{4}-[A-Z0-9]{4}-[A-Z0-9]{4}-[A-Z0-9]{4}$/)) {
                try {
                    // –ü—Ä–æ–≤–µ—Ä—è–µ–º, –Ω–µ –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω –ª–∏ —É–∂–µ —ç—Ç–æ—Ç –∫–ª—é—á
                    var keysFile = fso.BuildPath(shell.ExpandEnvironmentStrings("%TEMP%"), "bypass_keys.db");
                    var keyData = {};
                    
                    if (fso.FileExists(keysFile)) {
                        var file = fso.OpenTextFile(keysFile, 1);
                        var keysContent = file.ReadAll();
                        file.Close();
                        keyData = JSON.parse(keysContent);
                    }
                    
                    // –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å—É—â–µ—Å—Ç–≤—É–µ—Ç –ª–∏ –∫–ª—é—á –∏ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –∞–∫—Ç–∏–≤–∞—Ü–∏–π
                    if (keyData[proKey] && keyData[proKey].activations >= keyData[proKey].maxActivations) {
                        document.getElementById('proError').style.display = 'block';
                        document.getElementById('proError').innerText = '–î–æ—Å—Ç–∏–≥–Ω—É—Ç –ª–∏–º–∏—Ç –∞–∫—Ç–∏–≤–∞—Ü–∏–π –¥–ª—è —ç—Ç–æ–≥–æ –∫–ª—é—á–∞';
                        showNotification("–û—à–∏–±–∫–∞ –∞–∫—Ç–∏–≤–∞—Ü–∏–∏", "–î–æ—Å—Ç–∏–≥–Ω—É—Ç –ª–∏–º–∏—Ç –∞–∫—Ç–∏–≤–∞—Ü–∏–π –¥–ª—è —ç—Ç–æ–≥–æ –∫–ª—é—á–∞", "error");
                        return;
                    }
                    
                    // –ï—Å–ª–∏ –∫–ª—é—á –Ω–æ–≤—ã–π, –∏–Ω–∏—Ü–∏–∞–ª–∏–∑–∏—Ä—É–µ–º –µ–≥–æ
                    if (!keyData[proKey]) {
                        keyData[proKey] = {
                            activations: 0,
                            maxActivations: 3, // –ú–∞–∫—Å–∏–º—É–º 3 –∞–∫—Ç–∏–≤–∞—Ü–∏–∏ –Ω–∞ –∫–ª—é—á
                            created: new Date().getTime(),
                            expires: new Date().getTime() + (30 * 24 * 60 * 60 * 1000) // 30 –¥–Ω–µ–π
                        };
                    }
                    
                    // –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º —Å—á–µ—Ç—á–∏–∫ –∞–∫—Ç–∏–≤–∞—Ü–∏–π
                    keyData[proKey].activations++;
                    
                    // –°–æ—Ö—Ä–∞–Ω—è–µ–º –æ–±–Ω–æ–≤–ª–µ–Ω–Ω—ã–µ –¥–∞–Ω–Ω—ã–µ –∫–ª—é—á–µ–π
                    var file = fso.CreateTextFile(keysFile, true);
                    file.Write(JSON.stringify(keyData));
                    file.Close();
                    
                    // –°–æ—Ö—Ä–∞–Ω—è–µ–º PRO –∫–ª—é—á –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è
                    var proFile = fso.BuildPath(shell.ExpandEnvironmentStrings("%TEMP%"), "bypass_pro.key");
                    var userKeyData = {
                        key: proKey,
                        activated: new Date().getTime(),
                        expires: keyData[proKey].expires
                    };
                    
                    var userFile = fso.CreateTextFile(proFile, true);
                    userFile.Write(JSON.stringify(userKeyData));
                    userFile.Close();
                    
                    isProUser = true;
                    closeProModal();
                    updatePlansUI();
                    logMessage("PRO –≤–µ—Ä—Å–∏—è –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω–∞! –ö–ª—é—á: " + proKey, "pro");
                    showNotification("PRO –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω", "PRO –≤–µ—Ä—Å–∏—è —É—Å–ø–µ—à–Ω–æ –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω–∞! –°—Ä–æ–∫ –¥–µ–π—Å—Ç–≤–∏—è: 30 –¥–Ω–µ–π.", "pro");
                } catch (e) {
                    document.getElementById('proError').style.display = 'block';
                    document.getElementById('proError').innerText = '–û—à–∏–±–∫–∞ –∞–∫—Ç–∏–≤–∞—Ü–∏–∏: ' + e.message;
                    showNotification("–û—à–∏–±–∫–∞ –∞–∫—Ç–∏–≤–∞—Ü–∏–∏", "–û—à–∏–±–∫–∞ –ø—Ä–∏ –∞–∫—Ç–∏–≤–∞—Ü–∏–∏ PRO –∫–ª—é—á–∞: " + e.message, "error");
                }
            } else {
                document.getElementById('proError').style.display = 'block';
                document.getElementById('proError').innerText = '–ù–µ–≤–µ—Ä–Ω—ã–π —Ñ–æ—Ä–º–∞—Ç PRO –∫–ª—é—á–∞. –ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ —Ñ–æ—Ä–º–∞—Ç: XXXX-XXXX-XXXX-XXXX';
                showNotification("–û—à–∏–±–∫–∞", "–ù–µ–≤–µ—Ä–Ω—ã–π —Ñ–æ—Ä–º–∞—Ç PRO –∫–ª—é—á–∞. –ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ —Ñ–æ—Ä–º–∞—Ç: XXXX-XXXX-XXXX-XXXX", "error");
            }
        }

        // –°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –Ω–∞—Å—Ç—Ä–æ–µ–∫ –≤ —Ñ–∞–π–ª
        function saveSettings() {
            settings.theme = document.getElementById('theme').value;
            settings.autostart = document.getElementById('autostart').value;
            settings.encryption = document.getElementById('encryption').value;
            settings.autoclose = document.getElementById('autoclose').value;
            settings.stealthMode = document.getElementById('stealthMode').value;
            settings.autoUpdate = document.getElementById('autoUpdate').value;
            settings.notifications = document.getElementById('notifications').value;
            
            // –°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –Ω–∞—Å—Ç—Ä–æ–µ–∫
            try {
                var settingsFile = fso.BuildPath(shell.ExpandEnvironmentStrings("%TEMP%"), "bypass_settings.json");
                var file = fso.CreateTextFile(settingsFile, true);
                file.Write(JSON.stringify(settings));
                file.Close();
                
                // –ü—Ä–∏–º–µ–Ω—è–µ–º —Ç–µ–º—É
                currentTheme = settings.theme;
                applyTheme();
                updateThemeButton();
                
                logMessage('–ù–∞—Å—Ç—Ä–æ–π–∫–∏ —Å–æ—Ö—Ä–∞–Ω–µ–Ω—ã', "success");
                showNotification("–ù–∞—Å—Ç—Ä–æ–π–∫–∏ —Å–æ—Ö—Ä–∞–Ω–µ–Ω—ã", "–í–∞—à–∏ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ —É—Å–ø–µ—à–Ω–æ —Å–æ—Ö—Ä–∞–Ω–µ–Ω—ã.", "success");
                closeSettings();
            } catch (e) {
                logMessage('–û—à–∏–±–∫–∞ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è –Ω–∞—Å—Ç—Ä–æ–µ–∫: ' + e.message, "error");
                document.getElementById('settingsError').style.display = 'block';
                document.getElementById('settingsError').innerText = '–û—à–∏–±–∫–∞ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è: ' + e.message;
                showNotification("–û—à–∏–±–∫–∞", "–ù–µ —É–¥–∞–ª–æ—Å—å —Å–æ—Ö—Ä–∞–Ω–∏—Ç—å –Ω–∞—Å—Ç—Ä–æ–π–∫–∏: " + e.message, "error");
            }
        }

        // –ó–∞–≥—Ä—É–∑–∫–∞ –Ω–∞—Å—Ç—Ä–æ–µ–∫ –∏–∑ —Ñ–∞–π–ª–∞
        function loadSettings() {
            try {
                var settingsFile = fso.BuildPath(shell.ExpandEnvironmentStrings("%TEMP%"), "bypass_settings.json");
                
                if (fso.FileExists(settingsFile)) {
                    var file = fso.OpenTextFile(settingsFile, 1);
                    var settingsData = file.ReadAll();
                    file.Close();
                    
                    var loadedSettings = JSON.parse(settingsData);
                    // –°–æ—Ö—Ä–∞–Ω—è–µ–º —Ç–æ–ª—å–∫–æ –Ω–µ–æ–±—Ö–æ–¥–∏–º—ã–µ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏
                    settings.theme = loadedSettings.theme || 'dark';
                    settings.autostart = loadedSettings.autostart || 'off';
                    settings.encryption = loadedSettings.encryption || 'standard';
                    settings.autoclose = loadedSettings.autoclose || 'never';
                    settings.stealthMode = loadedSettings.stealthMode || 'off';
                    settings.autoUpdate = loadedSettings.autoUpdate || 'on';
                    settings.notifications = loadedSettings.notifications || 'all';
                    
                    currentTheme = settings.theme;
                }
            } catch (e) {
                logMessage("–û—à–∏–±–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏ –Ω–∞—Å—Ç—Ä–æ–µ–∫: " + e.message, "error");
            }
        }

        // –î–æ–±–∞–≤–ª–µ–Ω–∏–µ —Å–æ–æ–±—â–µ–Ω–∏—è –≤ –∫–æ–Ω—Å–æ–ª—å
        function logMessage(message, type) {
            var console = document.getElementById('console');
            var timestamp = new Date().toLocaleTimeString();
            var line = document.createElement('div');
            line.className = 'console-line' + (type ? ' ' + type : '');
            line.innerHTML = '[' + timestamp + '] ' + message;
            
            console.appendChild(line);
            console.scrollTop = console.scrollHeight;
        }
    </script>
