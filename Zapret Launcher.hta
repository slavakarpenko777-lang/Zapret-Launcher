        // –ü—Ä–æ–≤–µ—Ä–∫–∞ —Å—Ç–∞—Ç—É—Å–∞ PRO
        function checkProStatus() {
            try {
                var proFile = fso.BuildPath(shell.ExpandEnvironmentStrings("%TEMP%"), "bypass_pro.key");

                if (fso.FileExists(proFile)) {
                    var file = fso.OpenTextFile(proFile, 1);
                    var keyData = file.ReadAll();
                    file.Close();
                    
                    // –ü—Ä–æ–≤–µ—Ä—è–µ–º, –Ω–µ –∏—Å—Ç–µ–∫ –ª–∏ –∫–ª—é—á
                    var keyInfo = JSON.parse(keyData);
                    if (keyInfo.expires > new Date().getTime()) {
                        isProUser = true;
                        logMessage("PRO —Å—Ç–∞—Ç—É—Å: –∞–∫—Ç–∏–≤–µ–Ω", "pro");
                    } else {
                        // –£–¥–∞–ª—è–µ–º –ø—Ä–æ—Å—Ä–æ—á–µ–Ω–Ω—ã–π –∫–ª—é—á
                        fso.DeleteFile(proFile);
                        logMessage("PRO –∫–ª—é—á –∏—Å—Ç–µ–∫", "error");
                        showNotification("PRO –∏—Å—Ç–µ–∫", "–í–∞—à–∞ PRO –ø–æ–¥–ø–∏—Å–∫–∞ –∏—Å—Ç–µ–∫–ª–∞. –ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –ø—Ä–æ–¥–ª–∏—Ç–µ –µ–µ.", "warning");
                    }
                }
            } catch (e) {
                // PRO —Å—Ç–∞—Ç—É—Å –Ω–µ –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω
            }
